import java.util.*;

public class Prac {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        print(a,b);
    }
    public static void print(int a,int b){
        int temp=a;
        a=b;
        b=temp;
        System.out.println("a:" + a);
        System.out.println("b:" + b);
    }
}
