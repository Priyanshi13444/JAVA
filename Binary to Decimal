import java.util.*;

public class Prac {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int bin=sc.nextInt();
        System.out.println(bindec(bin));
    }
    public static int bindec(int bin){
        int pow=0;
        int dec=0;
        while(bin>0){
            int lastdigit= bin%10;
            dec=dec+ (lastdigit *(int) Math.pow(2,pow));
            pow++;
            bin /=10;
        }
        return dec;
    }
} 
